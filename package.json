    "@types/react-redux": "^7.1.9",
    "@types/react-select": "^3.0.14",
    "@types/styled-components": "^5.1.0",
    "autoprefixer": "^9.7.3",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "cssnano": "^4.1.10",
    "cssnano-preset-default": "^4.0.7",
    "del": "^5.1.0",
    "eslint": "^7.4.0",
    "gatsby": "^2.18.4",
    "gatsby-link": "^2.4.10",
    "gatsby-plugin-layout": "^1.1.18",
    "gatsby-plugin-postcss": "^2.1.16",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-react-redux": "^1.1.0-0",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-plugin-styled-components": "^3.2.3",
    "gatsby-plugin-ts": "^2.7.1",
    "gatsby-plugin-typescript": "^2.4.10",
