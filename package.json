{
  "name": "user-management",
  "private": true,
  "description": "A simple user management system",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "delete-compiled-js-files" : "node deleteCompiledJsFiles.js",
    "build": "yarn delete-compiled-js-files && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "open": "yarn clean && yarn build && yarn serve",
    "storybook": "tsc && start-storybook -p 6006 --ci",
    "storybook:build": "tsc && build-storybook",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
